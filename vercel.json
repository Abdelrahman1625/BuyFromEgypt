{
  "version": 2,
  "builds": [
    {
      "src": "src/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "https://YOUR_REPLIT_APP_URL/socket.io/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/src/vercel.ts"
    }
  ]
}
